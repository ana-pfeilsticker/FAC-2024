.data # dados guardados, constantes
helloText:
	.string "Hello World\n"
	
.text # a parte onde escreveremos os scripts, utilizaremos a main para indicar onde deve começar e o end para indicar a finalização
main: 


li a0, 1 #
la a1, helloText   #carrega o endereço da string
li a2, 12  #comprimento da string
li a7, 64          #código syscall para write
ecall   #executa a syscall

end:

li a7, 93   #código da syscall para exit
li a0, 0     #código de saída (0)
ecall       #executa a syscall