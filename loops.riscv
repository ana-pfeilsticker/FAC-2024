.data 
mensagemFim:
	.string"Fim do Loop\n"
barraN:
	.string"\n"
	
.text

main:
	li a7, 5
	ecall 
	mv t0, a0
	
loop:
	#imprime o valor do número adicionado pelo usuário
	mv a0, t0
	li a7, 1
	ecall
	
	li a0, 1
	la a1, barraN
	li a2, 1
	li a7, 64
	ecall
	
	
	
	addi t0, t0, -1	#decrementa uma unidade no loop
	
	bgtz t0, loop
	
	li a0, 1
	la a1, mensagemFim
	li a2 12
	li a7, 64
	ecall

end:

	li a7, 10
	ecall

	